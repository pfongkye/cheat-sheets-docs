{
  "index_name": "CheatSheets",
  "start_urls": [
    {
      "url": "https://pfongkye.github.io/docs/",
      "tags": [
        "docs"
      ]
    }
  ],
  "sitemap_urls": [
    "https://pfongkye.github.io/sitemap.xml"
  ],
  "sitemap_alternate_links": true,
  "stop_urls": [
    "/help",
    "/users"
  ],
  "selectors": {
    "lvl0": {
      "selector": "//*[contains(@class,'navGroups')]//*[contains(@class,'navListItemActive')]/preceding::h3[1]",
      "type": "xpath",
      "global": true,
      "default_value": "Blog"
    },
    "lvl1": ".post h1",
    "lvl2": ".post h2",
    "lvl3": ".post h3",
    "lvl4": ".post h4",
    "text": ".post article p, .post article li"
  },
  "selectors_exclude": [
    ".hash-link"
  ]
}
